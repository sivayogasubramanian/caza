{
  "name": "caza",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --no-error-on-unmatched-pattern --ext .js, .ts, .jsx, .tsx './**'",
    "lint:fix": "eslint --no-error-on-unmatched-pattern --ext .js, .ts, .jsx, .tsx --fix './**'",
    "postinstall": "husky install",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "docker-postgres": "docker run --rm -itd --name test-instance -p 5433:5432  -e POSTGRES_PASSWORD=password postgres && echo 'Allow a few seconds for the database to set up.'",
    "docker-postgres-migrate": "psql postgresql://postgres:password@localhost:5433 -c 'CREATE DATABASE caza' && yarn run prisma migrate reset",
    "docker-postgres-stop": "docker stop test-instance || true ",
    "test": "jest --watch --config=.jest.config.js --runInBand"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@prisma/client": "^4.3.1",
    "antd": "^4.23.1",
    "axios": "^0.27.2",
    "firebase": "^9.9.4",
    "firebase-admin": "^11.0.1",
    "immutable": "^4.1.0",
    "next": "12.2.5",
    "next-pwa": "^5.6.0",
    "plotly.js": "^2.14.0",
    "prisma": "^4.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-kawaii": "^0.17.0",
    "react-plotlyjs-ts": "^2.2.2",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.0",
    "@types/node": "18.7.14",
    "@types/plotly.js": "^2.12.3",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@types/react-kawaii": "^0.17.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.23.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.4",
    "husky": "^8.0.1",
    "jest": "^29.0.2",
    "jest-environment-jsdom": "^29.0.2",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "4.8.2"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
